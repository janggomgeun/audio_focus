{
  "name": "Audio Focus",
  "version": "0.1.0",
  "description": "Automatic Blur Effects to Inactive Tabs",
  "icons": {
    "256": "icon-main.png"
  },
  "author": "Woong Geun Jang",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background":{
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "js": ["content/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "256": "icon-main.png"
    },
    "default_title": "Audio Focus"
  },
  "minimum_chrome_version": "32.0.0",
  "web_accessible_resources": ["content/page.js"],
  "manifest_version": 2
}
